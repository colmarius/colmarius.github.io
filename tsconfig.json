{
	"extends": "astro/tsconfigs/strict",
	"include": [".astro/types.d.ts", "**/*"],
	"exclude": ["dist"],
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@components/*": ["./src/components/*"],
			"@types/*": ["./src/types/*"],
			"@assets/*": ["./src/assets/*"],
			"@layouts/*": ["./src/layouts/*"]
		}
	}
}
